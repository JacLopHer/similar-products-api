{
  "dashboard": {
    "id": null,
    "title": "K6 Performance Test Results - WORKING NOW",
    "description": "Dashboard FUNCIONAL con datos reales de K6",
    "tags": ["k6", "performance"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Total HTTP Requests",
        "type": "stat",
        "targets": [
          {
            "datasource": "myinfluxdb",
            "query": "SELECT count(value) FROM \"http_reqs\"",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Average Response Time (ms)",
        "type": "stat",
        "targets": [
          {
            "datasource": "myinfluxdb",
            "query": "SELECT mean(value) FROM \"http_req_duration\"",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 200}
              ]
            },
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Raw Data - HTTP Requests",
        "type": "table",
        "targets": [
          {
            "datasource": "myinfluxdb",
            "query": "SELECT time, scenario, status, value FROM \"http_reqs\" ORDER BY time DESC LIMIT 20",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8}
      }
    ]
  },
  "overwrite": true
}

